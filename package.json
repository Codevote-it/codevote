{
  "name": "codevote",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:moppedijk/codevote.git",
  "author": "<martijn.brug@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "nx exec -- ng",
    "prepare": "husky install",
    "generate": "yarn graphql-codegen",
    "lint:all": "eslint --fix --ext .js,.jsx,.ts .",
    "lint:server": "npx eslint apps/backend/src/ --fix",
    "start:server": "node dist/backend/main.js",
    "deploy:server": "nx exec -- yarn _deploy:server ",
    "prebuild:server": "rimraf dist && yarn generate",
    "build:server": "nx exec -- nest build",
    "serve:server": "yarn generate && nest start --watch",
    "lint:app": "npx eslint apps/frontend/src/ --fix",
    "start:app": "ng serve",
    "build:app": "nx exec -- ng build",
    "watch:app": "ng build --watch --configuration development",
    "test:app": "ng test",
    "_deploy:server": "build && apps/backend/build.sh && apps/backend/deploy.sh"
  },
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "graphql-request": "^4.0.0",
    "prismjs": "^1.28.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@google-cloud/datastore": "^6.6.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "apollo-server-express": "^3.6.2",
    "graphql": "^15",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-github2": "^0.1.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "short-unique-id": "^4.4.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/passport-github2": "^1.2.5",
    "@types/supertest": "^2.0.11",
    "dotenv": "^16.0.0",
    "jest": "^27.2.5",
    "localtunnel": "^2.0.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "nx": "16.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "nx": {
    "targets": {
      "deploy:server": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "build:server": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      },
      "build:app": {
        "outputs": [
          "{projectRoot}/dist"
        ]
      }
    }
  }
}
